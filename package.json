{
  "name": "react-anchor-without-hash",
  "version": "2.3.2",
  "description": "A anchor react component use url search string without hash.",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "scripts": {
    "demo:dev": "cd ./demo && npm run dev",
    "demo:build": "cd ./demo && npm run build",
    "demo:deploy": "gh-pages -d ./demo/dist",
    "build": "rollup -c",
    "commit": "npx git-cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "e2e": "jest -c jest-e2e.config.js",
    "test:unit": "jest --coverage",
    "test:updateSnapshot": "jest -u",
    "test:e2e": "start-server-and-test demo:dev http-get://localhost:9000 e2e",
    "test": "run-s test:unit test:e2e"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kwzm/react-anchor-without-hash.git"
  },
  "keywords": [
    "anchor",
    "react",
    "search",
    "hash",
    "without hash"
  ],
  "author": "kwzm",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kwzm/react-anchor-without-hash/issues"
  },
  "homepage": "https://github.com/kwzm/react-anchor-without-hash#readme",
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "commitizen": "^3.0.7",
    "coveralls": "^3.0.5",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "gh-pages": "^2.1.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "jest-puppeteer": "^4.3.0",
    "jest-transform-css": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^1.19.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-test-renderer": "^16.8.6",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.1",
    "start-server-and-test": "^1.9.2"
  },
  "browserslist": [
    ">0.1%",
    "not ie <= 8",
    "not op_mini all"
  ],
  "dependencies": {
    "@babel/runtime": "^7.4.2",
    "@ungap/url-search-params": "^0.1.2",
    "invariant": "^2.2.4",
    "prop-types": "^15.7.2",
    "query-string": "^6.4.2",
    "uniqid": "^5.0.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "bundleDependencies": [],
  "private": false,
  "files": [
    "lib",
    "es"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
